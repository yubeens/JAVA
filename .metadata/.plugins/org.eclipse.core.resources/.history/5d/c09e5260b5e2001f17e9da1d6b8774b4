package stringPro;

import java.util.Random;

public class MathEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=0; i<10; i++) {
			int n=(int)(Math.random()*30)+70;
			System.out.println(n);
		}
		System.out.println("-------------------");
		Random r=new Random();
		for(int i=0; i<10; i++) {
			int n=r.nextInt(30)+70; //50부터 100사이 값 출력
			System.out.println(n);
		}
	}

}
