package test01;

class Point {
	private int x,y;
	public Point(int x, int y) {
		this.x = x;
		this.y = y;
	}
}
public class ObjectEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Point p1 = new Point(2,3);
		Point p2 = new Point(3,4);
		System.out.println(p1.getClass());
	}

}
